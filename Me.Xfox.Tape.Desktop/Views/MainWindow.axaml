<Window xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:vm="using:Me.Xfox.Tape.Desktop.ViewModels" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="Me.Xfox.Tape.Desktop.Views.MainWindow" Icon="/Assets/avalonia-logo.ico" Title="Me.Xfox.Tape.Desktop">
    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
    <StackPanel>
        <TextBox Text="{Binding Username}" Watermark="Username"/>
        <TextBox Text="{Binding Password}" Watermark="Password" PasswordChar="*"/>
        <Button DockPanel.Dock="Bottom" Command="{Binding Compute}" Content="{Binding Text}"/>
    </StackPanel>
</Window>
